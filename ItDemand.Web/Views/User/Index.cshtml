<div class="container-fluid">
    <div class="row mt-3 mb-3">
        <div class="col-md-12">
            <h2 class="h4"><i class="fa-solid fa-user-gear"></i> Manage Users</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <table id="tblUsers" class="table w-100" style="font-size: 12px;">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>UserName</th>
                        <th>Email</th>
                        <th>Role</th>
                        <th>Active</th>
                        <th>Created</th>
                        <th>Last Modified</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>

@section scripts {
    <environment include="Development">
        <script src="~/js/manage-users.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/manage-users.min.js" asp-append-version="true"></script>
    </environment>
}